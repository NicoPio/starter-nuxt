{
  "name": "starter-nuxt",
  "type": "module",
  "private": true,
  "packageManager": "bun@1.3.3",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "typecheck": "nuxt typecheck",
    "lint": "eslint .",
    "test": "vitest",
    "test:unit": "vitest run test/unit",
    "test:nuxt": "vitest run --config vitest.nuxt.config.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "bun run test:unit && bun run test:nuxt && bun run test:e2e"
  },
  "dependencies": {
    "@nuxt/content": "^3.8.2",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/hints": "^1.0.0-alpha.2",
    "@nuxt/image": "^2.0.0",
    "@nuxt/scripts": "^0.13.0",
    "@nuxt/ui": "^4.2.1",
    "@tailwindcss/vite": "^4.1.17",
    "@unhead/vue": "^2.0.19",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.4.6",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "nuxt-auth-utils": "^0.5.26",
    "pg": "^8.16.3",
    "postgres": "^3.4.7",
    "stripe": "^20.0.0",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@iconify-json/heroicons": "^1.2.3",
    "@iconify-json/lucide": "^1.2.75",
    "@iconify-json/simple-icons": "^1.2.60",
    "@nuxt/test-utils": "^3.21.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/bcrypt": "^6.0.0",
    "@types/pg": "^8.15.6",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "happy-dom": "^15.11.7",
    "vitest": "^3.2.4"
  }
}
